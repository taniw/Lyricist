<h1>Home#index</h1>


<div class="jumbotron">
  <div class="container">
    <p>Find me in app/views/home/index.html.erb</p>


  <div class="col-lg-12">
    <div class="input-group">
      <input type="text" class="form-control search-query" aria-label="...">
      <div class="input-group-btn">
        <button class="btn btn-default search-song" type="button">Search</button>
      </div><!-- /btn-group -->
    </div><!-- /input-group --> 

    <div class="search-results"></div>

  </div><!-- /.col-lg-12 -->
  </div>
</div>


<script id="song-search-results-tpl" type="text/x-handlebars-template">
    <div class="container search-results">
      <ul class="results-list">
      {{#each data}}
        <li id="{{this.id}}">
          <div>
            <span>Song:</span> {{this.title}}
          </div>
          <div>
            <span>Artist:</span> {{this.artist.name}}
          </div>
        </li>
      {{/each}}
      </ul>    
    </div>
</script>



